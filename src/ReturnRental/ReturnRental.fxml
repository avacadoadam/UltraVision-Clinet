<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="640.0"
      xmlns="http://javafx.com/javafx/8.0.172-ea" fx:controller="ReturnRental.ReturnRentalController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">

            <Label layoutX="194.0" layoutY="37.0" text="Return Title">
                <font>
                    <Font size="45.0"/>
                </font>
            </Label>
            <Button layoutX="14.0" layoutY="15.0" mnemonicParsing="false" text="back" onMouseClicked="#back"/>
            <children>
                <Label fx:id="label" layoutX="78.0" layoutY="117.0" prefHeight="17.0" prefWidth="427.0" text="Scan IDs">
                    <font>
                        <Font size="28.0"/>
                    </font>
                </Label>
                <TextField fx:id="productID" layoutX="78.0" layoutY="250.0" prefHeight="51.0" prefWidth="444.0"
                           promptText="ProductID"/>
                <Button layoutX="78.0" layoutY="323.0" mnemonicParsing="false" onMouseClicked="#returnRental"
                        prefHeight="45.0" prefWidth="444.0" text="Finished"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
